@model WebApplication1.Models.DAL.post


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Delete</title>
</head>
<body>
    <h3>Ви дійсно хочете видалити дані про цю посаду?</h3>
    <div>
        
        <hr />
        <dl class="dl-horizontal">
            <dt>
                @Html.LabelForModel("Назва посади")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.title)
            </dd>
    
            <dt>
                @Html.LabelForModel("Оклад")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.salary)
            </dd>
    
        </dl>
    
        @using (Html.BeginForm()) {
            @Html.AntiForgeryToken()

    <div class="form-actions no-color btn-group">
        <input type="submit" value="Видалити" class="btn btn-default" />
        @Html.ActionLink("Повернутися до списку", "Index", null, new { @class = "btn btn-default" })
    </div>
        }
    </div>
</body>
</html>
